<%= content_tag(:div, id: "chart", class: "col-xs-10",
data: {daym1: day_of_week(Date.today - 1),
      daym2: day_of_week(Date.today - 2),
      daym3: day_of_week(Date.today - 3),
      daym4: day_of_week(Date.today - 4),
      daym5: day_of_week(Date.today - 5),
      daym6: day_of_week(Date.today - 6),
      today: @clicks_from_today,
      countm1: @clicks_from_1_day_ago,
      countm2: @clicks_from_2_days_ago,
      countm3: @clicks_from_3_days_ago,
      countm4: @clicks_from_4_days_ago,
      countm5: @clicks_from_5_days_ago,
      countm6: @clicks_from_6_days_ago
  }) do %>
<% end %>